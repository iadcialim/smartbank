{
  "name": "smartbank-api",
  "version": "1.0.0",
  "description": "SmartBank Australian Mobile Banking App - AWS SAM Backend API",
  "main": "src/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:contract": "jest tests/contract",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "sam:build": "sam build",
    "sam:local": "sam local start-api",
    "sam:deploy": "sam deploy",
    "sam:deploy:guided": "sam deploy --guided",
    "migrate:export": "cd scripts && npm run export",
    "migrate:transform": "cd scripts && npm run transform",
    "migrate:import": "cd scripts && npm run import",
    "migrate:validate": "cd scripts && npm run validate",
    "migrate:rollback": "cd scripts && npm run rollback"
  },
  "keywords": [
    "aws-sam",
    "typescript",
    "dynamodb",
    "cognito",
    "banking",
    "australia"
  ],
  "author": "SmartBank Team",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.478.0",
    "@aws-sdk/client-dynamodb": "^3.478.0",
    "@aws-sdk/lib-dynamodb": "^3.478.0",
    "@aws-sdk/client-kms": "^3.478.0",
    "aws-lambda": "^1.0.7",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "joi": "^17.11.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.130",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/uuid": "^9.0.7",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
